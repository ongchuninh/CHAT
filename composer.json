{
    "name": "gamota/gamotacms",
    "description": "The cms base on laravel.",
    "keywords": ["framework", "laravel", "phambinhcms"],
    "type": "project",
    "require": {
        "php": ">=5.6.4",
        "intervention/image": "^2.5",
        "ixudra/curl": "6.*",
        "jenssegers/agent": "^2.6",
        "kylekatarnls/laravel-carbon-2": "^1.0.0",
        "laravel/framework": "5.6.*",
        "laravel/tinker": "~1.0",
        "laravelcollective/html": "^5.3.0",
        "nesbot/carbon": "2.0.0-beta.2 as 1.25.0",
        "predis/predis": "~1.1@dev",
        "rap2hpoutre/laravel-log-viewer": "^1.6",
        "studio-42/elfinder": "^2.1.0"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~7.0"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/",
            "Gamota\\Dashboard\\": "gamota/dashboard/src/",
            "Gamota\\Appearance\\": "gamota/appearance/src/",
            "Gamota\\News\\": "gamota/news/src/",
            "Gamota\\Page\\": "gamota/page/src/",
            "Gamota\\Gallery\\": "gamota/gallery/src/",
            "Gamota\\Links\\": "gamota/links/src/",
            "Gamota\\Promise\\": "gamota/promise/src/",
            "Gamota\\Slider\\": "gamota/slider/src/",
            "Gamota\\FbComment\\": "gamota/fb-comment/src/",
            "Gamota\\CmsInstall\\": "gamota/cms-install/src/",
            "Gamota\\CmsDev\\": "gamota/cms-dev/src/",
            "Gamota\\Event\\": "gamota/event/src/",
            "Gamota\\Games\\": "gamota/games/src/",
            "Gamota\\Contacts\\": "gamota/contacts/src/",
            "Gamota\\Options\\": "gamota/options/src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    }
}